{
  "name": "repo-util",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "operate on remote repositories",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "repo-util": "src/repo-util-cli.mjs"
  },
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp"
  },
  "dependencies": {
    "aggregation-repository-provider": "^5.3.28",
    "bitbucket-repository-provider": "^5.1.6",
    "commander": "^10.0.1",
    "etag-cache-leveldb": "^2.0.2",
    "gitea-repository-provider": "^3.1.5",
    "github-repository-provider": "^8.0.16",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "repository-provider-cli-support": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "execa": "^7.1.1",
    "semantic-release": "^21.0.2"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/repo-util"
  },
  "bugs": {
    "url": "https://github.com/arlac77/repo-util/issues"
  },
  "homepage": "https://github.com/arlac77/repo-util#readme",
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-node-app"
    ]
  }
}

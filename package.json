{
  "name": "repo-util",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "operate on remote repositories",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "repo-util": "src/repo-util-cli.mjs"
  },
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp"
  },
  "dependencies": {
    "aggregation-repository-provider": "^3.2.54",
    "bitbucket-repository-provider": "^3.9.3",
    "commander": "^8.3.0",
    "gitea-repository-provider": "^1.16.32",
    "github-repository-provider": "^7.23.25",
    "local-repository-provider": "^7.0.101"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.10.0",
    "execa": "^5.1.1",
    "semantic-release": "^18.0.0"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/repo-util.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/repo-util/issues"
  },
  "homepage": "https://github.com/arlac77/repo-util#readme",
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-node-app"
    ]
  }
}
